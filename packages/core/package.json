{
  "name": "@fancywork/core",
  "private": true,
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "webpack --stats minimal",
    "lint": "eslint lib --ext .js,.jsx,.ts,.tsx && stylelint lib/**/*.scss && yarn run build",
    "pre-commit": "yarn run lint"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "*.ts",
          "*.tsx"
        ],
        "parserOptions": {
          "project": [
            "./tsconfig.json"
          ]
        }
      }
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-standard-scss"
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.16.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@types/react": "^17.0.32",
    "@types/react-dom": "^17.0.10",
    "@types/uuid": "^8.3.1",
    "antd": "^4.16.13",
    "css-loader": "^6.4.0",
    "mini-css-extract-plugin": "^2.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.43.3",
    "sass-loader": "^12.2.0",
    "ts-loader": "^9.2.6",
    "typescript": "4.3.5",
    "uuid": "^8.3.2",
    "webpack": "^5.59.1",
    "webpack-cli": "^4.9.1"
  }
}
